<%= @event.name %><br>
<% @event.tags.each do |tag| %>
  <p><%= link_to "##{tag.name}", tag %></p>
<% end %>
<%= @event.description %>
<p>
  From <%=@event.start_date%> - <%= @event.end_date %>
</p>
<%= link_to "Currently at #{@museum.name}", @museum %>
<br><br>
<%= image_tag @event.photo.url(:large) %>

<% if admin? %>
  <%= link_to "Add Event Tags", "#{@event.id}/tagging" %>
  <%= link_to "Edit Event", edit_event_path %>
<% end %>
